{
  "schemaVersion": 1,
  "id": "aofemotes",
  "version": "${version}",
  "name": "AOF Emotes",
  "description": "Discord style chat emotes for Minecraft chat. A fork of Fabric Emotes.",
  "authors": [
    "AlgidSpace",
    "TeamAOF"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/aof-emotes/"
  },
  "license": "MIT",
  "icon": "assets/aofemotes/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "ninjaphenix.aofemotes.Main"
    ]
  },
  "mixins": [
    "aofemotes.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric-resource-loader-v0": ">=0.5.3",
    "minecraft": ">=1.19",
    "java": ">=17"
  },
  "breaks": {
    "fabricemotes": "*"
  }
}
