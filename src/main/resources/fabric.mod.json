{
  "schemaVersion": 1,
  "id": "aofemotes",
  "version": "${version}",
  "name": "AOF Emotes",
  "description": "Discord style chat emotes for Minecraft chat. A fork of Fabric Emotes.",
  "authors": [
    "AlgidSpace",
    "TeamAOF"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/aof-emotes/",
    "sources": "https://github.com/Ellemes/aof-emotes/",
    "issues": "https://github.com/Ellemes/aof-emotes/issues/"
  },
  "license": "MIT",
  "icon": "assets/aofemotes/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "ellemes.aofemotes.Main"
    ]
  },
  "mixins": [
    "aofemotes.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "fabric-resource-loader-v0": ">=0.4.12",
    "minecraft": "~1.18.1",
    "java": ">=17"
  },
  "breaks": {
    "fabricemotes": "*"
  }
}
